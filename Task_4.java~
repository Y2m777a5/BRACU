import java.util.Scanner;
public class Task_4 {
  
  
  public static void main(String[] args) 
  { 
    Scanner sc = new Scanner(Syatem.in);
    System.out.println("Please give the value of minutes: ");
    int min = sc.nextInt();
    
    int years = min/(60*24*365);
    int days = min/(60*24);
    System.out.println(min+"minutes"+years+" years and "+days+" days");
  }
  
  /* ADD YOUR CODE HERE */
  
}
