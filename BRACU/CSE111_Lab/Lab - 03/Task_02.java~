import java.util.Scanner;
public class Task_02 {
  
  
  public static void main(String[] args) { 
    Scanner sc = new Scanner(System.in);
    String str = sc.next();
    int len = str.length();
    int nstr = "";
    for(int i=0; i<len; i++){
      int lttr = (int)str.charAt(i);
      if(lttr>=(int)'A' && lttr<=()int'z'){
        lttr-=32;
      }
      nstr+=(char)lttr;
    }
    System.out.println("nstr >"+ nstr);
  }
  
  /* ADD YOUR CODE HERE */
  
}
