
public class Toy {
  public String name;
  public int price;
  
  public Toy(String name, int price) {
    this.name=name;
    this.price=price;
  }
  
  public int updatePrice(int price){
    this.price=price;
  }
  
  public int updateName(int name){
    this.name=name;
  }
  
  public void showPrice(){
    System.out.pritnln("price: "+price+" Taka");
  }
}
