
public class PlusMinus 
{
    public static void main(String[] args )
    {
      int n =4, sum=0;
      int k=1;
      for(n=0; n<4; n++)
      {
        //minus
        int i=2;
        while(i!=0)
        {
           sum-=k;
           i--;
        }

        //plus
        int j=2;
        while(j!=0)
        {
            sum+=k;
            j--;
        }
      }
      System.out.println(sum);
    }
}