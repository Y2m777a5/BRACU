import java.lang.Math;
class Untitled_x
{
    public static void main(String[] args)
    {
        int a = 8;
        int b = 3;
        double c = Math.sqrt(((a/2)*(a/2))+(b*b));
        double area = ((a*c)+(a*b));
        double circumference = (c*6);    
        System.out.println(area);
        System.out.println(circumference);
            
        }
}