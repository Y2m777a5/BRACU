import java.util.Scanner;
public class Task_5 {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int inp =sc.nextInt();
        int n=2;
        
        for(int i=0;i<inp;i++){
            while(true){
              
           int sum=0; int j=2;
            
            while(j<n){
               int r=n%j;
                if(r==0){sum=sum+j;}
                j++;}
            
            if((sum+1)==n){
                System.out.println(n);break;}
            
            n++;
            }
            n++;
        }
        sc.close();
        
    }
}