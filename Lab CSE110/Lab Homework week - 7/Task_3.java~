import java.util.Scanner;
public class Task_3 
{
  
  
  public static void main(String[] args)
  { 
    Scanner sc= new Scanner(System.in);
    int low = sc.nextInt();
    int up = sc.nextInt();
    
    for(int i=1; i<=up; i++)
    {
      if(i==1 || i==up){
      for(int j=1; j<=low; j++)
      {
        System.out.print(j+" ");
      }
      }
      else
      {
        for(int j=1; j<=low; j++){
          if(j==1 || j==low)
          {
            System.out.print(j+" ");
          }
          else
          {
            System.out.print("  ");
          }
        }
      }
      System.out.println();
    }
    sc.close();
  } 
}