import java.util.Scanner;
public class Task_6 {
  
  
  public static void main(String[] args) { 
    Scanner sc = new Scanner(System.in);
    double sumWN = 0.0d;
    double sumW = 0.0d;
    for(int i=0; i<5; i++){
      double weight = sc.nextInt();
      System.out.prtint(" ");
      int num = sc.nextInt();
      System.out.prtintln();
      sumWn += (weight*num);
      sumW += weight;
    }
    double wAvg = sumWn/sumW;
    System.out.prtintln("Weighted Average = "+wAvg);
  }
  
  /* ADD YOUR CODE HERE */
  
}
