
public class Task_01 {
  
  
  public static void main(String[] args) { 
    
    //Task_A;
    int i=2;
    System.out.print(i+", ");
    while(i<50){
      i+=4;
      if(i==50){
        System.out.println(i);
      }
      else{
        System.out.print(i+", ");
      }
    }
    
    //Task_B
    int j=1;
    int k=2;
    System.out.print(j+", ");
    while(j<120){
      j+=k;
      if(j==120){
        System.out.println(j);
      }
      else{
        System.out.print(j+", ");
      }
      k++;
    }
    
  }
  
  /* ADD YOUR CODE HERE */
  
}
